<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mealAware.app">
    <!-- The INTERNET permission is required for development. Specifically,
         the Flutter tool needs it to communicate with the running application
         to allow setting breakpoints, to provide hot reload, etc.
    -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="com.android.vending.BILLING" />
     <application>
        <!-- Add the following in the application section -->
        <meta-data
            android:name="com.android.vending.billing.IInAppBillingService"
            android:value="com.google.android.gms.boss.api.BillingApi" />
        <meta-data
            android:name="com.android.billingclient.api.version"
            android:value="4.0.0" />
        
            
        <!-- Other configurations and activities -->
    </application>
</manifest>
